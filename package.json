{
  "name": "typeio",
  "version": "1.0.6",
  "description": "A custom multiselect UI widget based on twitter type ahead plugin",
  "main": "dist/typeio.js",
  "scripts": {
    "build:uglify": "npm run clean && npm run sass && npm run jshint && uglifyjs ./src/typeio.js > ./dist/typeio.min.js",
    "build": "npm run clean && npm run sass && npm run jshint && cp ./src/typeio.js ./dist/typeio.js",
    "clean": "./node_modules/rimraf/bin.js dist/* *.log* ",
    "browserify": "./node_modules/browserify/bin/cmd.js ./src/**/*.js | uglifyjs > ./dist/typeio.min.js",
    "browserify:debug": "./node_modules/browserify/bin/cmd.js --debug ./src/typeio.js -o ./dist/typeio.js",
    "sass": "mkdir -p dist/assets/styles && sass src/assets/styles/styles.scss dist/assets/styles/styles.css && rsync -a src/assets/ dist/assets/ --exclude styles",
    "jshint": "./node_modules/jshint/bin/jshint ./src/**",
    "test": "./node_modules/karma/bin/karma start ./karma.conf.js --singleRun=true"
  },
  "author": {
    "name": "Michigan Institute for Clinican & Health Research",
    "url": "https://www.michr.umich.edu"
  },
  "contributors": [
    {
      "name": "Melih Gunal"
    },
    {
      "name": "Raden Tonev"
    },
    {
      "name": "Tianyun Zhang"
    },
    {
      "name": "Aalap Doshi"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "keywords": ["autocomplete", "typeahead", "multiselect", "dropdown", "select", "search", "input", "form"],
  "devDependencies": {
    "browserify": "^13.0.1",
    "browserify-istanbul": "^0.2.1",
    "jasmine-core": "^2.4.1",
    "jshint": "^2.9.2",
    "karma": "^1.1.0",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "rimraf": "^2.5.2",
    "sass": "^0.5.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "jquery": ">=1.7",
    "typeahead.js": "^0.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michr-cri/TypeIO"
  }
}
